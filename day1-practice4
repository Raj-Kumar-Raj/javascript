// wriete the code to assign grade accoroding o their marks 
let num = prompt("enter the marks");
    if(num<=100 && num>=80){
        console.log("A");
    }

    else if(num<=89 && num>=70){
        console.log("B");
    
    }

    else if(num<=69 && num>=60){
        console.log("C");
    
    }

    else if(num<=59 && num>=50){
        console.log("D");
    
    }

    else {
        console.log("F");
    
    }

    
