 let arry=[30,40,50];
 console.log(arry.every((el)=>(el%2==0)));

 console.log("............................................................");
 // write function that to find the min no in an array 
 let mim =arr.reduce((smll,el)=>{
    if(smll >el){
        return el;
    }
    else{
        return smll;
    }
 });

 console.log(mim);
