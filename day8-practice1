// maximum element of the  array using reduce
let arr=[2, 3, 4 ,5,6];
let marks =arr.reduce((max,el)=>{
if(max<el){
    return el;
}else{
    return max;
}
});

 console.log(marks);

 // check if all the element of the array is multiple of 10 or not 

